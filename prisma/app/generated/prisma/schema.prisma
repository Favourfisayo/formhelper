generator client {
  provider = "prisma-client-js"
  output   = "./app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FormSession {
  id                String   @id @default(uuid())
  fields            Json
  currentFieldIndex Int      @default(0)
  session_lang      String
  sessionStarted    Boolean  @default(false)
  createdAt         DateTime @default(now())
  fileName          String
  fileType          String
  fileUrl           String
}
